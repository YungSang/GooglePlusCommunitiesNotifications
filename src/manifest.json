{
	"name"             : "__MSG_my_name__",
	"version"          : "0.17.6",
	"manifest_version" : 2,
	"description"      : "__MSG_my_desc__",
	"icons"            : {
		"16"             : "icons/icon16.png",
		"48"             : "icons/icon48.png",
		"128"            : "icons/icon128.png"
	},
	"default_locale"   : "en",
	"browser_action"   : {
		"default_icon"   : "icons/icon19.png",
		"default_title"  : "__MSG_my_name__",
		"default_popup"  : "popup.html"
	},
	"background"       : {
		"scripts"        : [
			"js/jquery-1.6.4.min.js",
			"js/main.js"
		]
	},
	"minimum_opera_version" : "15.0",
	"options_page"     : "options.html",
	"permissions"      : [
		"tabs",
		"cookies",
		"https://plus.google.com/*"
	],
	"incognito"        : "split",
	"update_url"       : "http://yungsang.com/google+/communitiesnotifications/opera/update.xml",
	"homepage_url"     : "http://yungsang.com/google+/communitiesnotifications/",
	"web_accessible_resources" : [
		"sandbox.html"
	],
	"sandbox"          : {
		"pages"          : [
			"sandbox.html"
		]
	}
}
